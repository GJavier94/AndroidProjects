<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.contentproviderclient">

    <uses-permission
        android:name="android.permission.READ_USER_DICTIONARY"
        android:requiredFeature="true" />
    <uses-permission
        android:name="android.permission.WRITE_USER_DICTIONARY"
        android:requiredFeature="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ContentProviderClient">
        <activity
            android:name=".activities.InsertActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.RetrieveActivity"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>